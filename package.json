{
  "name": "iot-app",
  "producName": "IOT APP",
  "version": "2.1.6",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "Yusuf Dwi",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev ",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:all": "npm run build:win && npm run build:mac && npm run build:linux",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux",
    "release": "electron-builder --publish=always",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@emotion/cache": "^11.13.1",
    "@emotion/react": "^11.13.3",
    "@emotion/serialize": "^1.3.2",
    "@emotion/utils": "^1.4.1",
    "@mantine/carousel": "^7.13.2",
    "@mantine/core": "^7.13.2",
    "@mantine/dates": "^7.13.2",
    "@mantine/dropzone": "^7.13.2",
    "@mantine/emotion": "^7.13.2",
    "@mantine/form": "^7.13.2",
    "@mantine/hooks": "^7.13.2",
    "@mantine/notifications": "^7.13.2",
    "@tabler/icons-react": "^3.19.0",
    "@tanstack/react-query": "^5.59.0",
    "@tanstack/react-query-devtools": "^5.59.0",
    "abort-controller": "^3.0.0",
    "axios": "^1.7.7",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "electron-log": "^5.2.0",
    "electron-progressbar": "^2.2.1",
    "electron-store": "^10.0.0",
    "electron-updater": "^6.3.9",
    "embla-carousel-autoplay": "^8.3.0",
    "esbuild-plugin-react-virtualized": "^1.0.4",
    "esbuild-wasm": "^0.24.0",
    "eslint": "^8.57.1",
    "got": "^14.4.2",
    "react-qr-code": "^2.0.15",
    "react-router-dom": "^6.26.2",
    "react-spring": "^9.7.4",
    "react-virtualized": "^9.22.5",
    "recharts": "^2.12.7",
    "use-sound": "^4.0.3",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@tanstack/eslint-plugin-query": "^5.59.2",
    "@types/debounce": "^1.2.4",
    "@types/electron-progressbar": "^1.2.7",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@types/react-virtualized": "^9.21.30",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "@vitejs/plugin-react": "^4.3.2",
    "electron": "^32.1.2",
    "electron-builder": "^25.1.7",
    "electron-vite": "^2.3.0",
    "esbuild": "^0.24.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "npm-force-resolutions": "^0.0.10",
    "postcss": "^8.4.47",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.8"
  },
  "overrides": {
    "@electron/get": {
      "got": "^11.8.5"
    },
    "@types/electron-progressbar": {
      "electron": "^32.1.2"
    }
  },
  "build": {
    "appId": "com.electron.app",
    "productName": "IOT APP",
    "directories": {
      "buildResources": "build"
    },
    "files": [
      "!**/.vscode/*",
      "!src/*",
      "!electron.vite.config.{js,ts,mjs,cjs}",
      "!{.eslintignore,.eslintrc.cjs,.prettierignore,.prettier.yaml,dev-app-update.yml,CHANGELOG.md,README.md}",
      "!{.env,.env.*,.npmrc,pnpm-lock.yaml}",
      "!{tsconfig.json,tsconfig.node.json,tsconfig.web.json}"
    ],
    "publish": {
      "provider": "github",
      "owner": "dwcahyo23",
      "repo": "electron-dashboard",
      "releaseType": "release"
    }
  }
}
