{
  "name": "electron-dashboard",
  "version": "2.0.6",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "Yusuf Dwi",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev ",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:all": "npm run build:win && npm run build:mac && npm run build:linux",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux",
    "release": "electron-builder --publish=always",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@mantine/carousel": "^7.11.1",
    "@mantine/core": "^7.11.1",
    "@mantine/dates": "^7.11.1",
    "@mantine/dropzone": "^7.11.1",
    "@mantine/form": "^7.11.1",
    "@mantine/hooks": "^7.11.1",
    "@tabler/icons-react": "^3.9.0",
    "@tanstack/react-query": "^5.50.1",
    "@tanstack/react-query-devtools": "^5.50.1",
    "abort-controller": "^3.0.0",
    "axios": "^1.7.2",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.11",
    "electron-log": "^5.1.5",
    "electron-store": "^9.0.0",
    "electron-updater": "^6.1.7",
    "embla-carousel-autoplay": "^7.1.0",
    "esbuild-plugin-react-virtualized": "^1.0.4",
    "esbuild-wasm": "^0.24.0",
    "react-qr-code": "^2.0.15",
    "react-router-dom": "^6.24.1",
    "react-spring": "^9.7.4",
    "react-virtualized": "^9.22.5",
    "recharts": "^2.12.7",
    "use-sound": "^4.0.3",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@tanstack/eslint-plugin-query": "^5.50.1",
    "@types/debounce": "^1.2.4",
    "@types/node": "^20.14.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-virtualized": "^9.21.30",
    "@vitejs/plugin-react": "^4.3.1",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.3.0",
    "esbuild": "^0.24.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "npm-force-resolutions": "^0.0.10",
    "postcss": "^8.4.39",
    "postcss-preset-mantine": "^1.15.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.5.2",
    "vite": "^5.3.1"
  },
  "build": {
    "appId": "com.electron.app",
    "productName": "electron-dashboard",
    "directories": {
      "buildResources": "build"
    },
    "files": [
      "!**/.vscode/*",
      "!src/*",
      "!electron.vite.config.{js,ts,mjs,cjs}",
      "!{.eslintignore,.eslintrc.cjs,.prettierignore,.prettier.yaml,dev-app-update.yml,CHANGELOG.md,README.md}",
      "!{.env,.env.*,.npmrc,pnpm-lock.yaml}",
      "!{tsconfig.json,tsconfig.node.json,tsconfig.web.json}"
    ],
    "publish": {
      "provider": "github",
      "owner": "dwcahyo23",
      "repo": "electron-dashboard",
      "releaseType": "release"
    }
  }
}
